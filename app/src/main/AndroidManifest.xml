        </activity>
        <activity
            android:name=".Nuevo_platillo"
            android:exported="false">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Listar_platillos_admin" />
        </activity>
        <activity
            android:name=".Listar_platillos_user"
            android:exported="false" />
        <activity
            android:name=".Listar_platillos_admin"
            android:exported="false"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>